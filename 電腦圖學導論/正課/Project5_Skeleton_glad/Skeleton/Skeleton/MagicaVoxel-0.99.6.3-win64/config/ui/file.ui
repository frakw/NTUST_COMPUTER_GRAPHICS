		{
			switch(class = 'panel')(def='h2' selected = '1' path = '1 1' text = 'Shader' flex = '1' next = '3')
			button(def = 'bn-grey' text = '^42' textFont = 'icon' onClick = 'xs'            delayed = '1' textSize = '10 10')(size = '35' margin = '0 0 4 4' next = '1' corner = '5' radius = '3')(hint = 'Execute Shader')
			button(def = 'bn-grey' text = '^38' textFont = 'icon' onClick = 'set xs_shader' delayed = '1' textSize = '12 12')(size = '35' margin = '0 0 4 4' corner = '10' radius = '3')(hint = 'Refresh Shader')
		}
		
		hbox(size = '28' margin = '9 0 0 0')(hint = 'Iteration Number') ::
		{
			label(def = 'h3' text = 'Iteration')(flex = '1')
			textField(def = 'tf-sub' id = 'xs_iter' onEnter = 'set xs_iter' text = '1' maxLength = '3')(size = '20')
			resizer(class = 'int')(range = '1 64' step = '1' space = '4')(size = '8' prev = '3')
		}

