vbox(margin = '6 6 0 0') ::
{
	vbox(id = 'menu_model')(flex = '1') ::
	{
		switch(class = 'panel')(def='h2' selected = '1' text = 'Project' size = '30')
		
		layer(def = 'layer-dark')(flex = '1') ::
		{
			list(def = 'fileList' id = 'file' root = [[$/vox]] ext = "vox" onSelect = 'open' )(hint = 'Project List')
		}
	}

	vbox(id = 'menu_patt' flex = '1' hidden = '1') ::
	{
		switch(class = 'panel')(def='h2' path = '0 2' selected = '1' text = 'Pattern')(size = '30')
				
		hbox(size = '20') ::
		{
			sp(flex = '1')
			button(def = 'bn-grey' onClick = 'set pattlib -1' text = '^9664' textSize = '9 9' textFont = 'symbol' delayed = '0' repeated = '1')(corner = '5' radius = '3' size = '20' next = '1')(hint = 'Previous Pattern [3]')
			button(def = 'bn-grey' onClick = 'set pattlib -2' text = '^9654' textSize = '9 9' textFont = 'symbol' delayed = '0' repeated = '1')(corner = '10' radius = '3' size = '20' next = '8')(hint = 'Next Pattern [4]')

			button(def = 'bn-grey' onClick = 'onewpatt' text = '^10' textFont = 'icon' textSize = '10 10' delayed = '1')(corner = '15' radius = '3' size = '20' next = '8')(hint = 'Add Pattern in the Scene')
		
			button(def = 'bn-grey' text = '^73' textFont = 'icon' textSize = '14 14' onClick = 's_patt 1')(corner = '5' radius = '3')(size = '20' next = '1')
			(hint = 'Create Reference Patterns from Selected Objects')
		
			button(def = 'bn-grey' text = '^12' textFont = 'icon' textSize = '14 14' onClick = 's_patt')(corner = '10' radius = '3')(size = '20')
			(hint = 'Create Patterns from Selected Voxels/Objects')
			sp(flex = '1')
		}
				
		layer(bgColor = '0 0 0 0' resize = '4' size = '125' max = '250' flex = '-1') ::
		{
			preview(id = 'preview' flex = '1' margin = '6 6 6 6')(hint = 'Pattern Preview')
		}

		hbox(margin = '9 6 0 0' size = '28')(hint = 'Match Palette Color') ::
		{
			label(def = 'h3' text = 'Match Color')(flex = '1' next = '3')
			switch(def = 'sw-check' onClick = 'set option_import patt_match' selected = '0')(size = '16')
		}
		
		layer(def = 'layer-dark')(flex = '1') ::
		{
			list(def = 'fileList' id = 'file_patt' root = [[$/vox]] ext = 'vox' onSelect = 'i_patt')(hint = 'Pattern List')
		}
	}

